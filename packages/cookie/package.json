{
  "name": "@adonisjs/cookie",
  "version": "1.1.0",
  "description": "Cookie parser for AdonisJs framework",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/config",
    "build/index.js",
    "build/index.d.ts"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "doc": "typedoc",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "commit": "git-cz"
  },
  "keywords": [
    "http",
    "cookie",
    "adonisjs"
  ],
  "author": "virk",
  "homepage": "https://adonisjs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/adonisjs/adonis-framework/tree/master/packages/cookie"
  },
  "license": "MIT",
  "dependencies": {
    "cookie": "^0.3.1",
    "cookie-signature": "^1.1.0"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
