{
  "name": "@adonisjs/request",
  "version": "1.3.0",
  "description": "Facade over Node.js HTTP req object",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/config",
    "build/index.js",
    "build/index.d.ts"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "doc": "typedoc",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "commit": "git-cz"
  },
  "keywords": [
    "http",
    "req"
  ],
  "author": "virk",
  "homepage": "https://adonisjs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/adonisjs/adonis-framework/tree/master/packages/request"
  },
  "license": "MIT",
  "devDependencies": {
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@adonisjs/cookie": "^1.1.0",
    "accepts": "^1.3.5",
    "cuid": "^2.1.6",
    "fresh": "^0.5.2",
    "get-value": "^3.0.1",
    "lodash": "^4.17.11",
    "macroable": "^2.0.2",
    "proxy-addr": "^2.0.5",
    "qs": "^6.7.0",
    "type-is": "^1.6.16"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
