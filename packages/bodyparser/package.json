{
  "name": "@adonisjs/bodyparser",
  "version": "2.2.3",
  "description": "AdonisJs body parser to read and parse HTTP request bodies",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/config",
    "build/index.js",
    "build/index.d.ts",
    "build/lib"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "doc": "typedoc",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [],
  "author": "virk",
  "homepage": "https://adonisjs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/adonisjs/adonis-framework/tree/master/packages/bodyparser"
  },
  "license": "MIT",
  "peerDependencies": {
    "@adonisjs/request": "1.x",
    "@adonisjs/router": "1.x"
  },
  "dependencies": {
    "@adonisjs/utils": "^1.4.3",
    "bytes": "^3.1.0",
    "co-body": "^6.0.0",
    "end-of-stream": "^1.4.1",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "media-typer": "^1.1.0",
    "multiparty": "^4.2.1",
    "uuid": "^3.3.2"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "devDependencies": {
    "@adonisjs/dev-utils": "^1.3.0",
    "@adonisjs/request": "^1.2.0",
    "@adonisjs/router": "^1.5.8",
    "supertest": "^4.0.2"
  }
}
