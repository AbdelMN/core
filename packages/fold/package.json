{
  "name": "@adonisjs/fold",
  "version": "5.0.9",
  "description": "Dependency manager and ioc container for your next NodeJs application",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.js",
    "build/index.d.ts"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "doc": "typedoc",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "clear-module": "^3.2.0",
    "fs-extra": "^7.0.1"
  },
  "keywords": [
    "ioc",
    "dependency",
    "injection",
    "service",
    "provider",
    "autoloading"
  ],
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-framework/issues"
  },
  "homepage": "https://adonisjs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/adonisjs/adonis-framework/tree/master/packages/fold"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "dependencies": {
    "@adonisjs/utils": "^1.4.4"
  }
}
