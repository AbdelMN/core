{
  "name": "@adonisjs/core",
  "version": "2.5.0",
  "description": "Adonisjs core",
  "main": "build/providers/AppProvider.js",
  "files": [
    "build/adonis-typings",
    "build/config",
    "build/providers",
    "build/src",
    "build/instructions.js"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc && copyfiles config/* build",
    "build": "npm run compile",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build"
  },
  "types": "./build/adonis-typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-framework.git"
  },
  "keywords": [
    "adonisjs",
    "framework",
    "mvc"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-framework/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-framework#readme",
  "devDependencies": {
    "@adonisjs/ace": "^6.7.0",
    "@adonisjs/assembler": "^1.1.0",
    "@adonisjs/fold": "^6.2.3",
    "@adonisjs/mrm-preset": "^2.1.0",
    "@adonisjs/sink": "^2.3.1",
    "@poppinss/dev-utils": "^1.0.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^12.12.12",
    "@types/supertest": "^2.0.8",
    "commitizen": "^4.0.3",
    "copyfiles": "^2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "del-cli": "^3.0.0",
    "doctoc": "^1.4.0",
    "husky": "^3.1.0",
    "japa": "^3.0.1",
    "mrm": "^1.2.3",
    "np": "^5.1.3",
    "proxy-addr": "^2.0.5",
    "strip-ansi": "^6.0.0",
    "supertest": "^4.0.2",
    "test-console": "^1.1.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.2",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@adonisjs/application": "^1.2.1",
    "@adonisjs/config": "^1.0.10",
    "@adonisjs/encryption": "^1.0.2",
    "@adonisjs/env": "^1.0.10",
    "@adonisjs/events": "^1.0.5",
    "@adonisjs/hash": "^1.0.8",
    "@adonisjs/http-server": "^1.5.1",
    "@adonisjs/logger": "^1.1.6",
    "@adonisjs/profiler": "^1.2.4",
    "@poppinss/utils": "^2.1.0",
    "@types/find-package-json": "^1.1.0",
    "@types/proxy-addr": "^2.0.0",
    "find-package-json": "^1.2.0",
    "macroable": "^3.0.0",
    "ms": "^2.1.2",
    "on-finished": "^2.3.0",
    "simple-encryptor": "^3.0.0"
  },
  "peerDependencies": {
    "@adonisjs/fold": "^6.0.0",
    "@adonisjs/ace": "^6.0.0"
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "templates": {
      "basePath": "./build/config",
      "config": [
        "app.txt",
        "cors.txt"
      ]
    },
    "types": "@adonisjs/core"
  }
}
