{
  "name": "@adonisjs/core",
  "version": "2.7.3",
  "description": "Adonisjs core",
  "main": "build/providers/AppProvider.js",
  "files": [
    "build/adonis-typings",
    "build/config",
    "build/providers",
    "build/src",
    "build/instructions.js"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "lint": "eslint . --ext=.ts",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc && copyfiles config/* build",
    "build": "npm run compile",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build"
  },
  "types": "./build/adonis-typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-framework.git"
  },
  "keywords": [
    "adonisjs",
    "framework",
    "mvc"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-framework/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-framework#readme",
  "devDependencies": {
    "@adonisjs/ace": "^6.7.5",
    "@adonisjs/assembler": "^1.2.6",
    "@adonisjs/fold": "^6.2.3",
    "@adonisjs/mrm-preset": "^2.2.3",
    "@adonisjs/sink": "^2.4.2",
    "@poppinss/dev-utils": "^1.0.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "commitizen": "^4.0.3",
    "copyfiles": "^2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "del-cli": "^3.0.0",
    "doctoc": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-adonis": "^1.0.5",
    "husky": "^4.0.7",
    "japa": "^3.0.1",
    "mrm": "^2.0.2",
    "np": "^5.2.1",
    "proxy-addr": "^2.0.5",
    "strip-ansi": "^6.0.0",
    "supertest": "^4.0.2",
    "test-console": "^1.1.0",
    "ts-node": "^8.6.1",
    "typescript": "^3.7.4",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@adonisjs/application": "^1.3.3",
    "@adonisjs/bodyparser": "^3.1.0",
    "@adonisjs/config": "^1.0.11",
    "@adonisjs/encryption": "^1.0.3",
    "@adonisjs/env": "^1.0.11",
    "@adonisjs/events": "^1.0.6",
    "@adonisjs/hash": "^1.0.10",
    "@adonisjs/http-server": "^1.6.4",
    "@adonisjs/logger": "^1.1.7",
    "@adonisjs/profiler": "^1.2.4",
    "@adonisjs/validator": "^6.1.1",
    "@poppinss/utils": "^2.1.1",
    "@types/find-package-json": "^1.1.1",
    "@types/proxy-addr": "^2.0.0",
    "find-package-json": "^1.2.0",
    "macroable": "^4.0.1",
    "ms": "^2.1.2",
    "on-finished": "^2.3.0",
    "simple-encryptor": "^3.0.0"
  },
  "peerDependencies": {
    "@adonisjs/fold": "^6.0.0",
    "@adonisjs/ace": "^6.0.0"
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "templates": {
      "basePath": "./build/config",
      "config": [
        "cors.txt"
      ]
    },
    "types": "@adonisjs/core",
    "providers": [
      "@adonisjs/core"
    ]
  }
}
