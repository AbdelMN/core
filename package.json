{
  "name": "@adonisjs/core",
  "version": "2.0.16",
  "description": "Adonisjs core",
  "main": "build/index.js",
  "files": [
    "build/providers",
    "build/adonis-typings",
    "build/src",
    "build/config"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc && copyfiles config/* build",
    "build": "npm run compile",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build"
  },
  "types": "./build/adonis-typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-framework.git"
  },
  "keywords": [
    "adonisjs",
    "framework",
    "mvc"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-framework/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-framework#readme",
  "devDependencies": {
    "@adonisjs/fold": "^6.1.6",
    "@adonisjs/mrm-preset": "^2.1.0",
    "@adonisjs/sink": "^2.1.9",
    "@poppinss/dev-utils": "^1.0.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^12.7.4",
    "@types/supertest": "^2.0.8",
    "commitizen": "^4.0.3",
    "copyfiles": "^2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "del-cli": "^2.0.0",
    "doctoc": "^1.4.0",
    "husky": "^3.0.5",
    "japa": "^3.0.1",
    "mrm": "^1.2.2",
    "np": "^5.0.3",
    "proxy-addr": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.6.2",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@poppinss/application": "^1.0.10",
    "@poppinss/config": "^1.0.7",
    "@poppinss/env": "^1.0.6",
    "@poppinss/events": "^1.0.2",
    "@poppinss/hash": "^1.0.4",
    "@poppinss/http-server": "^1.2.3",
    "@poppinss/logger": "^1.1.3",
    "@poppinss/profiler": "^1.2.0",
    "@poppinss/utils": "^1.0.9",
    "@types/proxy-addr": "^2.0.0",
    "find-package-json": "^1.2.0",
    "ms": "^2.1.2",
    "on-finished": "^2.3.0",
    "simple-encryptor": "^3.0.0"
  },
  "peerDependencies": {
    "@adonisjs/fold": "^6.x.x",
    "youch": "^2.x.x",
    "youch-terminal": "^1.x.x"
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "adonisjs": {
    "templates": {
      "basePath": "./build/config",
      "config": [
        "app.txt",
        "cors.txt",
        "hash.txt"
      ]
    }
  }
}
