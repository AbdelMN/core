{
  "name": "@adonisjs/core",
  "version": "5.0.0-preview-rc-1.5",
  "description": "Adonisjs core",
  "main": "build/providers/AppProvider.js",
  "files": [
    "build/adonis-typings",
    "build/templates",
    "build/commands",
    "build/providers",
    "build/src",
    "build/instructions.js"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "lint": "eslint . --ext=.ts",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc && copyfiles templates/**/* build",
    "build": "npm run compile",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build"
  },
  "types": "./build/adonis-typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-framework.git"
  },
  "keywords": [
    "adonisjs",
    "framework",
    "mvc"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-framework/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-framework#readme",
  "devDependencies": {
    "@adonisjs/ace": "^6.9.2",
    "@adonisjs/assembler": "^2.0.3",
    "@adonisjs/fold": "^6.3.5",
    "@adonisjs/mrm-preset": "^2.3.0",
    "@adonisjs/sink": "^3.0.0",
    "@poppinss/dev-utils": "^1.0.6",
    "@types/node": "^13.13.4",
    "@types/supertest": "^2.0.8",
    "commitizen": "^4.0.4",
    "copyfiles": "^2.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "del-cli": "^3.0.0",
    "doctoc": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-adonis": "^1.0.9",
    "husky": "^4.2.5",
    "japa": "^3.0.1",
    "mrm": "^2.3.0",
    "np": "^5.2.1",
    "reflect-metadata": "^0.1.13",
    "strip-ansi": "^6.0.0",
    "supertest": "^4.0.2",
    "test-console": "^1.1.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@adonisjs/application": "^1.3.13",
    "@adonisjs/bodyparser": "^4.0.2",
    "@adonisjs/config": "^1.0.14",
    "@adonisjs/encryption": "^2.0.2",
    "@adonisjs/env": "^1.0.14",
    "@adonisjs/events": "^2.0.1",
    "@adonisjs/hash": "^2.0.4",
    "@adonisjs/http-server": "^2.1.0",
    "@adonisjs/logger": "^2.0.4",
    "@adonisjs/profiler": "^3.0.1",
    "@adonisjs/validator": "^7.0.3",
    "@poppinss/utils": "^2.2.6",
    "@types/find-package-json": "^1.1.1",
    "cli-table3": "^0.6.0",
    "find-package-json": "^1.2.0",
    "semver": "^7.3.2",
    "serve-static": "^1.14.1"
  },
  "peerDependencies": {
    "@adonisjs/fold": "^6.0.0",
    "@adonisjs/ace": "^6.0.0"
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "templates": {
      "basePath": "./build/templates",
      "config": [
        {
          "src": "config/bodyparser.txt",
          "dest": "bodyparser"
        },
        {
          "src": "config/cors.txt",
          "dest": "cors"
        },
        {
          "src": "config/hash.txt",
          "dest": "hash"
        }
      ],
      "contracts": [
        {
          "src": "contracts/events.txt",
          "dest": "events"
        },
        {
          "src": "contracts/hash.txt",
          "dest": "hash"
        }
      ]
    },
    "commands": [
      "@adonisjs/core/build/commands"
    ],
    "types": "@adonisjs/core",
    "providers": [
      "@adonisjs/core"
    ]
  }
}
